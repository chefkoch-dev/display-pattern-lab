{% if not document.isRoot %}<h{{ document.depth }} id="{{document.title}}">{{ document.title }}</h{{ document.depth }}>{% endif %}

{% if document.documentation %}
    {% include "@lab/documents/" ~ document.documentation.type ~ ".html.twig" with {document: document.documentation} %}
{% endif %}

{% for child in document.children %}

    {% include "@lab/documents/" ~ child.type ~ ".html.twig" with {document: child} %}

{% endfor %}